@{
    ViewData["Title"] = "寄付者一覧";
}

<!DOCTYPE html>
<html lang="ja">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - MathSet_Recycling_Admin</title>
</head>
<body>
    <div>
        <div class="container">
            <h4 class="text-center mt-5 mb-3">寄付者一覧</h4>
            <table class="table table-striped table-bordered border-dark">
                <thead>
                    <tr class="table-info font-size table-bordered border-dark align-middle">
                        <th scope="col" class="text-center">寄付者番号</th>
                        <th scope="col" class="text-center">寄付者氏名</th>
                        <th scope="col" class="text-center">寄付日</th>
                        <th scope="col" class="text-center"></th>
                    </tr>
                </thead>
                <tbody class="align-middle">
                    <tr class="font-size">
                        <td scope="row"></td>
                        <td class="font-size"></td>
                        <td></td>
                        <td class="text-center ">
                            <input type="button" value="詳細" class="btn btn-dark btn-sm">
                        </td>
                    </tr>
                </tbody>
            </table>
        </div>
    </div>
</body>
</html>